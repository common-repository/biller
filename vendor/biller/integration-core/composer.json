{
  "name": "biller/integration-core",
  "description": "Biller integrations core library",
  "version": "1.0.9",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=5.6",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Biller\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Biller\\Console\\": "console"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "codacy/coverage": "dev-master",
    "symfony/console": "^4.4"
  },
  "config": {
    "platform": {
      "php": "7.2"
    }
  }
}
