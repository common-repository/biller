{
  "name": "biller/woocommerce",
  "description": "NOTYD WooCommerce Integration",
  "type": "library",
  "version": "1.0.14",
  "require": {
    "biller/integration-core": "^1.0.5",
    "php": ">=5.6",
    "ext-json": "*"
  },
  "autoload": {
    "classmap": [
      "includes"
    ]
  },
  "repositories": [
    {
      "type": "path",
      "url":  "../../integration-core"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "config": {
    "platform": {
      "php": "7.2"
    }
  },
  "autoload-dev": {
    "classmap": [
      "includes/", "tests/", "vendor/biller/integration-core/src/"
    ]
  }
}
